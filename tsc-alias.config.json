  "resolveFullPaths": true,
  "replacers": {
    "@/*": "dist/*",
    "@config/*": "dist/config/*",
    "@middleware/*": "dist/middleware/*",
    "@clients/*": "dist/clients/*",
    "@aggregators/*": "dist/aggregators/*",
    "@routes/*": "dist/routes/*",
    "@services/*": "dist/services/*",
    "@types/*": "dist/types/*",
    "@utils/*": "dist/utils/*",
    "@observability": "dist/observability",
    "@observability/*": "dist/observability/*"
  }
}
